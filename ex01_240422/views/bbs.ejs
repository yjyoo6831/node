<h1><%=title %></h1>
<div id="posts"></div>
<!--게시판 템플릿-->
<script id="temp" type="x-handlebars-template">
	<table class="table table-striped table-hover">
		{{#each .}}
			<tr>
				<td>{{id}}</td>
				<td>{{title}}</td>
			</tr>
			<tr>
				<td colspan=2>{{body}}</td>
			</tr>
		{{/each}}
	</table>
</script>


<script>
	function getList(){
		fetch('https://jsonplaceholder.typicode.com/posts/')
    	.then(json => {
			console.log(json);
			const temp=Handlebars.compile($("#temp").html());
			$("#posts").html(temp(json));
		});
	
	}
	getList();
</script>
<div style="text-align: center;">
	<button id="prev" class="btn btn-outline-success">이전</button>
	<span id="pn"></span>
	<button id="next" class="btn btn-outline-success">다음</button>
</div>
